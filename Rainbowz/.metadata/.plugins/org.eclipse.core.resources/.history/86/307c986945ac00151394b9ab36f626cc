		int b = text.length();
		
		for(int i=1;i<=b;i++){
			charzart.add(text.substring(i-1,i));
		}
		
		for(int z=0;z<charzart.size();z++){
			System.out.println(charzart.get(z));
		}
		
		
		//hexa array
				
		int a = text.length()-1;
		
		for(int z=0;z<charzart.size();z++){
			
			if(charzart.get(z).toString().matches(" ")){
				a--;
			}
		}
				
		
		int numb = total/a;
				
		for(int x=0;x<=16777216;x=x+numb){
					hexa.add(x);
		}
		
		
		
